root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.py]
indent_size = 4
[*.js]
indent_size = 4
[*.jsx]
indent_size = 4
[*.txt]
indent_size = 4
[*.cmake]
indent_size = 4

[*.md]
trim_trailing_whitespace = false
